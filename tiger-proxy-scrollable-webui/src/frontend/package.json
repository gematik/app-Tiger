    "@vueuse/core": "13.9.0",
    "bootstrap": "5.3.8",
    "fflate": "0.8.2",
    "highlight.js": "11.11.1",
    "patch-package": "8.0.0",
    "simple-syntax-highlighter": "3.1.1",
    "vitest": "3.2.4",
    "vue": "3.5.21",
    "vue-router": "4.5.1",
    "vue-virtual-scroller": "2.0.0-beta.8"
