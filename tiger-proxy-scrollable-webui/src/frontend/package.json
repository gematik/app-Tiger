{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:detached": "vue-tsc -b && vite build --mode=detached",
    "build:all": "npm run build:detached && npm run build",
