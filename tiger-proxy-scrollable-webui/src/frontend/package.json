    "lint": "eslint .",
    "lint:fix": "eslint . --fix && npm run format",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "format": "prettier --write src/",
    "postinstall": "patch-package"
  },
  "overrides": {
    "ansi-regex": "5.0.1",
    "ansi-styles": "5.2.0",
    "backslash": "0.2.0",
    "chalk": "5.6.2",
    "chalk-template": "1.1.2",
    "color-convert": "3.1.0",
    "color-name": "2.0.0",
    "color-string": "2.1.0",
    "debug": "4.4.1",
    "error-ex": "1.3.2",
    "has-ansi": "6.0.2",
    "is-arrayish": "0.3.2",
    "simple-swizzle": "0.2.2",
    "slice-ansi": "7.1.2",
    "strip-ansi": "6.0.1",
